[color]
	ui = true

[alias]
	edit-unmerged = "!f() { git diff --name-status --diff-filter=U | cut -f2 ; }; ${EDITOR:=subl} `f`"
	add-unmerged = "!f() { git diff --name-status --diff-filter=U | cut -f2 ; }; git add `f`"

	br = branch
	ci = commit -a
	co = checkout
	ds = diff --stat
	last = log -1 HEAD
	st = status
	unstage = reset HEAD --

[push]
	default = simple

[credential]
	helper = osxkeychain

